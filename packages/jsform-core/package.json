{
    "name": "jsform-core",
    "version": "1.0.0",
    "description": "",
    "main": "dist/index..js",
    "directories": {
        "doc": "docs",
        "test": "tests"
    },
    "scripts": {
        "test": "mocha ./tests/**/*.spec.js --require babel-register --reporter=list --watch",
        "dev": "NODE_ENV=development webpack --config=webpack/webpack.dev.config.js --watch",
        "prod": "NODE_ENV=production webpack --config=webpack/webpack.prod.config.js",
        "build": "npm run prod;npm run dev;npm run tsdoc;",
        "tree": "tree -L 3 -I  'node_modules'",
        "tsdoc": "typedoc --out docz src"
    },
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@types/invariant": "^2.2.29",
        "@types/json-schema": "^7.0.3",
        "@types/node": "^11.13.6"
    },
    "dependencies": {
        "tslib": "^1.9.3"
    }
}
